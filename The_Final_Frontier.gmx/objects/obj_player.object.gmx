    speed = 0
    }

//Health Low Warning    
if(global.hp &lt; 30) {

    global.healthlow = 1
    sprite_index = spr_playerlowshield
    }
else if(global.hp &lt; 60) {

    sprite_index = spr_playermidshield
    global.healthlow = 0
    }

else {

    sprite_index = spr_player
    global.healthlow = 0
    }
    
switch(global.healthlow)
    {
    case 0: sound_stop(sound_lowshield) break
    case 1: if (alarm[0] &lt; 0) {alarm[0] = 40} break
    }

//Player Death    
