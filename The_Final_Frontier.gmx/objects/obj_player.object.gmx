    sound_play(sound_destroy);
    instance_create(0,0,obj_gameover);
    effect_create_above(ef_firework, x, y, 2, c_green);
    instance_destroy();
}

if(global.current_lvl == 11){
    instance_create(0,0,obj_gamewin);
    instance_destroy();
    }

</string>
